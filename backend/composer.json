{
    "name": "secret-hitler/backend",
    "description": "Backend for Secret Hitler Multi-Device Game",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "cboden/ratchet": "^0.4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "SecretHitler\\": "classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SecretHitler\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "start": "php -S localhost:8000",
        "websocket": "php websocket/server.php"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
